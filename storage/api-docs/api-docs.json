{
    "openapi": "3.0.0",
    "info": {
        "title": "API's Room 911",
        "version": "1.0"
    },
    "servers": [
        {
            "url": "http://127.0.0.1:8000"
        }
    ],
    "paths": {
        "/api/v1/auth/authentication": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Ingreso de usuarios",
                "operationId": "c757f9366ac8fe96b9b957998b54cb98",
                "parameters": [
                    {
                        "name": "email",
                        "in": "query",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "password",
                        "in": "query",
                        "schema": {
                            "type": "password"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "example": {
                                        "access_token": "Acceso correcto",
                                        "token_type": "",
                                        "expires_at": "",
                                        "user": []
                                    }
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Failed",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "example": {
                                        "message": "Mensaje de error"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/v1/export/sample_file": {
            "get": {
                "tags": [
                    "Exports"
                ],
                "summary": "Exportar documento de muestra",
                "operationId": "f87cc486ecb5652c74984dc651b25043",
                "responses": {
                    "200": {
                        "description": "{Download Sample File}"
                    }
                },
                "security": [
                    {
                        "bearer_token": []
                    }
                ]
            }
        },
        "/api/v1/export/income_record": {
            "get": {
                "tags": [
                    "Exports"
                ],
                "summary": "Exportar registro de ingresos de empleados",
                "operationId": "4885e4803b875a4c97c42b4205d46f82",
                "responses": {
                    "200": {
                        "description": "{Download Income Record}"
                    }
                },
                "security": [
                    {
                        "bearer_token": []
                    }
                ]
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearer_token": {
                "type": "apiKey",
                "description": "Enter token in format (Bearer <token>)",
                "name": "Authorization",
                "in": "header"
            }
        }
    }
}